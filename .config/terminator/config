    font = Hack Nerd Font Mono 13
    foreground_color = "#ffffff"
    show_titlebar = False
    use_system_font = False
    use_theme_colors = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
